{
    "schemaVersion": 1,
    "id": "${mod_id}",
    "version": "${mod_version}",
    "name": "${mod_name}",
    "description": "${mod_description}",
    "authors": ["tobynguyen27"],
    "contact": {
        "email": "hi@tobynguyen.dev",
        "homepage": "https://github.com/tobynguyen27/Defineds-Blocks",
        "issues": "https://github.com/tobynguyen27/Defineds-Blocks/issues",
        "sources": "https://github.com/tobynguyen27/Defineds-Blocks"
    },
    "license": "${mod_license}",
    "icon": "assets/definedsblocks/icon.png",
    "environment": "*",
    "entrypoints": {
        "main": [
            {
                "value": "dev.tobynguyen27.definedsblocks.DefinedsBlocks",
                "adapter": "kotlin"
            }
        ],
        "client": [
            {
                "value": "dev.tobynguyen27.definedsblocks.client.DBClient",
                "adapter": "kotlin"
            }
        ],
        "fabric-datagen": [
            {
                "value": "dev.tobynguyen27.definedsblocks.DBDataGen",
                "adapter": "kotlin"
            }
        ]
    },
    "mixins": ["definedsblocks.mixins.json"],
    "depends": {
        "fabricloader": ">=${loader_version}",
        "minecraft": "~${minecraft_version}",
        "java": ">=17",
        "fabric-api": "*",
        "fabric-language-kotlin": "*",
        "sense": ">=1.0.0",
        "chipped": "*"
    }
}
